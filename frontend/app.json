{
  "expo": {
    "name": "Career AI",
    "slug": "career-ai",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#1F1C2F"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.careerai.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#1F1C2F"
      },
      "package": "com.careerai.app"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "career-ai"
  }
}
